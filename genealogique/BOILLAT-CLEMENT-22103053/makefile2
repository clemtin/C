# Makefile
CC = gcc
SRCS = testgenea.c identite.c genea.c
OBJS = $(SRCS:.c=.o)
CFLAGS = -Wall -Wextra
TARGET = testgenea

# Règle de construction de l'exécutable
$(TARGET): $(OBJS)
	$(CC) $(CFLAGS) -o $@ $^

# Règle de construction des fichiers objets (.o)
%.o: %.c
	$(CC) $(CFLAGS) -c -o $@ $<

# Règle pour nettoyer les fichiers objets et l'exécutable
clean:
	-rm -f $(OBJS) $(TARGET)